.ui-loader{ background:rgba(255,255,255,.7); position:fixed; left:0; top:0; right:0; bottom:0;  z-index: 1010;}
.ui-loader {/*display: none;*/}

.ui-loader .spinner {display: block;position: fixed;z-index: 1031;top: 50%;right: 50%;}
.ui-loader .spinner-icon {width: 44px;height: 44px;box-sizing: border-box;border: solid 3px transparent;border-top-color: #038F1B;border-left-color: #999999;border-radius: 50%;
animation: nprogress-spinner 400ms linear infinite;
-webkit-animation: nprogress-spinner 400ms linear infinite;
}

